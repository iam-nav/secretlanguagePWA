    body {
      color: #FFFFFF;
      background: #091624;
    }

    .solar-bg {
      background-image: url('https://sln-storage.s3.us-east-2.amazonaws.com/icons/solar-system-background.png') !important;
      width: 100vw !important;
      background-position: center;
      background-repeat: no-repeat;
      background-size: cover;

      mix-blend-mode: normal !important;
    }

    .wave-bg {
      width: 100% !important;
      background: none !important;
    }

    .main-header {
      font-family: 'SignPainter';
      font-style: italic;
      font-weight: 600;
      font-size: 55px;
      line-height: 78px;
      /* identical to box height, or 142% */

      text-align: center;
      letter-spacing: 0.1375px;

      color: #FFFFFF;
    }

    .border-icon {
      width: 300px !important;
      height: 300px !important;

      border-radius: 50% !important;
    }

    .image-section {
      display: flex !important;
      justify-content: center !important;
      align-items: center !important;

      margin: 0 auto !important;
      width: 441px !important;
      height: 441px !important;

      background-image: url('https://sln-storage.s3.us-east-2.amazonaws.com/icons/border-icon.svg') !important;
      background-position: center;
      background-repeat: no-repeat;
      background-size: cover;
    }

    .name-age {
      font-family: Times;
      font-style: normal;
      font-weight: bold;
      font-size: 50px;
      line-height: 57px;
      text-align: center;
      text-transform: uppercase;

      color: #AE9463;
    }

    .location {
      font-family: Gilroy;
      font-style: normal;
      font-weight: normal;
      font-size: 34px;
      line-height: 34px;
      /* or 101% */

      text-align: center;

      color: #FFFFFF;
    }

    .description-section {
