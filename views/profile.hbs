      height: 250px !important;
    }

    .sl-name-description {
      font-family: Gilroy;
      font-style: normal;
      font-weight: normal;
      font-size: 25px;
      line-height: 40px;
      /* identical to box height, or 160% */

      text-align: center;

      color: #FFFFFF;

      mix-blend-mode: normal;
      opacity: 0.5;
    }

    .sl-name {
      font-family: AppleMyungjo;
      font-style: normal;
      font-weight: normal;
      font-size: 50px;
      line-height: 35px;
      /* identical to box height, or 70% */

      text-align: center;
      letter-spacing: 0.125px;
      text-transform: uppercase;

      color: #FFFFFF;
    }

    .famous-years {
      width: 40%;
      font-family: Avenir;
      font-style: normal;
      font-weight: 500;
      font-size: 20px;
      line-height: 25px;
      /* or 120% */

      text-align: center;

      color: #FFFFFF;
    }

    .section-title {
      max-width: 85% !important;
      font-family: Gilroy;
      font-style: normal;
      font-weight: 800;
      font-size: 36px;
      line-height: 45px;
      /* identical to box height */

      text-align: center;
      text-transform: uppercase;

      color: #AE9463;
    }

    .section-description {
      width: 85% !important;
      font-family: Gilroy;
      font-style: normal;
      font-weight: normal;
      font-size: 24px;
      line-height: 28px;
      /* or 117% */

      text-align: justify;

      color: #FFFFFF;

      mix-blend-mode: normal;
      opacity: 0.7;
    }

    .footer {
      display: flex !important;
      justify-content: center !important;
      align-items: center !important;
      flex-direction: column !important;

      width: 100% !important;
      height: 200px !important;
      background: #050E17 !important;

      font-family: Avenir;
      font-style: normal;
      font-weight: normal;
      font-size: 24px;
      line-height: 31px;
      /* or 129% */

      text-align: center;

      color: #FFFFFF;
    }

    @media screen and (max-width: 576px) {
      .main-header {
        font-size: 34px;
        line-height: 55px;
      }

      .image-section {
        width: 350px !important;
        height: 350px !important;

        background-image: url('https://sln-storage.s3.us-east-2.amazonaws.com/icons/border-icon.svg') !important;
        background-position: center;
        background-repeat: no-repeat;
        background-size: cover;
      }

      .border-icon {
        width: 240px !important;
        height: 240px !important;
      }

      .name-age {
        font-size: 32px !important;
        line-height: 40px !important;
      }

      .location {
        font-size: 26px !important;
        line-height: 26px !important;
      }

      .sl-icon {
        width: 200px !important;
        height: 200px !important;
      }

      .sl-name-description {
        font-size: 16px !important;
        line-height: 30px !important;
      }

      .sl-name {
        font-size: 22px !important;
        line-height: 29px !important;
      }

      .famous-years {
        width: 90% !important;
        font-size: 14px !important;
        line-height: 18px !important;
      }

      .section-title {
        font-size: 28px !important;
        line-height: 36px !important;
      }

      .section-description {
        font-size: 15px !important;
        line-height: 24px !important;
      }

      .footer {
        height: 150px !important;
        font-size: 13px;
        line-height: 14px;
      }

      .footer-line {
        width: 80% !important;
      }
    }

    @media screen and (max-width: 1000px) {
      .sl-name {
        font-size: 30px;
        line-height: 34px;
      }

      .famous-years {
        width: 70% !important;
      }
    }
  </style>
</head>

